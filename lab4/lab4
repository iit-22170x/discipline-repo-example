#1.a
^[ab]{2}a[ab]*b[ab]{4}$
#1.b
^(ab|ba|aa|bb)*$
#1.c
^(?!.*ab)[ab]*$
#1.d
^(?!.*aab)[ab]*$
#1.e
^(?!.*bb)[abc]*$
#1.f
^(?!.*b[abc]a)[abc]*$
#1.g
^(?!.*a[ab])[abc]*[bc]$
#2
^(?!.*[3-9]\d{2})(?!.*2[6-9]\d)(?!.*25[6-9])((\d{1,3}\.){3}\d{1,3})$
#3
^(?!.*\(\))(?!.*\([+*\/])(?!.*[+\-*\/]\))(?!^[+*\/)])(?!.*[+\-*\/(]$)(?!.*[+\-*\/]{2})([\d+\-*\/()]*)$